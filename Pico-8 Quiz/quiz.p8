pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
x = 64
y = 64
speed

function _update()
	
	if (btn(0)) and x < 0 then 
		x=x-speed
	
	
	if (btn(1)) and x > 118 then 
		x=x+speed
	end
	
	if (btn(2)) and y >0 
		y=y-speed
	end
	
	if (btn(3)) and y < 118 then 
		y=y+speed
	end
	


function _draw()
	--rectfill(0,0,127,127,5)
	spr(1,x)
end
