__lua__
function _init()
	x = 0
end


function _update()
	if (btn(0)) then
		x = x+1
	end
	if (btn(1)) then
		x = x-1
	end	

end


function _draw()
	cls()
	camera(x, 0)
	map(0, 0, 0, 0)
	spr(1, 64, 64)

end
